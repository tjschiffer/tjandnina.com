<div class="tj-navigation
            tj--z-index-10
            tj--width-full
            <%# isOverlay %>
            tj--position-absolute
            tj--position-top
            <%/ isOverlay %>
            <%^ isOverlay %>
            tj--position-relative
            <%/ isOverlay %>">
  <div class="tj--hide-tablet tj--height-full tj--width-full">
    <nav class="tj-grid-flex
                tj-grid-flex--justify-center
                tj--height-full
                tj--width-full
                tj--font-size-20
                tj--font-weight-light">
      <%# navigationLinks %>
      <a href="<% linkAddress %>"
         class="tj-navigation__link
              tj-grid-flex
              tj-grid-flex--align-center
              tj--padding-hor-3
              tj--text-decoration-none
              tj--font-white">
        <div>{{ <% linkName %> }}</div>
      </a>
      <%/ navigationLinks %>
    </nav>
    <div class="tj--position-absolute
                tj--position-top
                tj--position-right
                tj--height-full">
      <%& navigationLanguageChooser %>
    </div>
  </div>
  <div class="tj--hide-desktop tj--height-full tj--width-full tj--border-bottom-white-1">
    <div class="tj-navigation__language-chooser-mob
                tj--display-none
                tj--position-absolute
                tj--position-top
                tj--position-left
                tj--height-full">
      <%& navigationLanguageChooser %>
    </div>
    <nav class="tj-navigation__links-mob
                tj--display-none
                tj--position-absolute
                tj--width-full
                tj--font-size-20
                tj--font-weight-light
                tj--border-bottom-white-1">
      <%# navigationLinks %>
      <a href="<% linkAddress %>"
         class="tj-navigation__link-mob
                tj-grid-flex
                tj-grid-flex--align-center
                tj-grid-flex--justify-center
                tj--text-decoration-none
                tj--font-white">
        <div>{{ <% linkName %> }}</div>
      </a>
      <%/ navigationLinks %>
      <div class="tj--position-absolute
                  tj--position-top
                  tj--width-full
                  tj--height-full
                  tj--overflow-hidden
                  tj--z-index-neg-1">
        <div class="tj--navigation__background-nav-links
                    tj--position-absolute
                    tj--width-full"
             style="background-image: url('/img/paris.jpg')"></div>
      </div>
    </nav>
  </div>
  <%^ isOverlay %>
  <div class="tj--position-absolute
              tj--position-top
              tj--width-full
              tj--height-full
              tj--overflow-hidden
              tj--z-index-neg-1">
    <div class="tj--navigation__background
                tj--width-full"
                style="background-image: url('/img/paris.jpg')"></div>
  </div>
  <%/ isOverlay %>
</div>
