<div class="tj--bg-floral-white tj--padding-vert-5">
  <div class="tj--max-width-1020 tj--margin-hor-auto tj--min-height-100vh-minus-nav tj-grid-flex tj-grid-flex--wrap-mob">
    <%# getPhotosColumns %>
    <div class="tj-grid-flex__cell-1-3 tj-grid-flex__cell-1-1--mob tj--padding-hor-1">
      <%# . %>
      <div class="tj--padding-1 tj--margin-bottom-1 tj--bg-white">
        <picture>
          <source srcset="<% . %>.webp" type="image/webp">
          <source srcset="<% . %>.jpg" type="image/jpeg">
          <img src="<% . %>.jpg"  class="tj--width-full tj--display-block" alt="picture of us">
        </picture>
      </div>
      <%/ . %>
    </div>
    <%/ getPhotosColumns %>
  </div>
</div>