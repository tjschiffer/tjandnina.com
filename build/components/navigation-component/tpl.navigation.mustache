<div data-tj-navigation
    class="tj-navigation
           tj--z-index-10
           tj--width-full
           <%# isOverlay %>
           tj--position-absolute
           tj--position-top
           <%/ isOverlay %>
           <%^ isOverlay %>
           tj--position-relative
           <%/ isOverlay %>">
  <div class="tj--hide-tablet tj--height-full tj--width-full">
    <nav class="tj-grid-flex
                tj-grid-flex--justify-center
                tj--height-full
                tj--width-full
                tj--font-size-20
                tj--font-weight-light">
      <%# navigationLinks %>
      <a href="<% linkAddress %>"
         class="tj-navigation__link
              tj-grid-flex
              tj-grid-flex--align-center
              tj--padding-hor-3
              tj--text-decoration-none
              tj--font-white">
        <div class="tj-grid-flex
                    tj-grid-flex--align-center
                    tj--height-full
                    tj--position-relative">
          <div>{{ <% linkName %> }}</div>
        <%# isActive %>
        <div class="tj--position-absolute
                    tj--width-full
                    tj--position-left
                    tj--position-bottom
                    tj--border-bottom-white-3"></div>
        <%/ isActive %>
        </div>

      </a>
      <%/ navigationLinks %>
    </nav>
    <div class="tj--position-absolute
                tj--position-top
                tj--position-right
                tj--height-full
                tj--padding-bottom-1">
      <%& navigationLanguageChooser %>
    </div>
  </div>
  <div class="tj--hide-desktop tj--height-full tj--width-full tj--border-bottom-white-1">
    <div class="tj-navigation__language-chooser-mob
                tj--display-none
                tj--position-absolute
                tj--position-top
                tj--position-left
                tj--height-full">
      <%& navigationLanguageChooser %>
    </div>
    <div onclick="(function(){ $('[data-tj-navigation]').toggleClass('tj-navigation--open'); })();"
        class="tj--position-absolute
               tj--position-right
               tj--position-top
               tj--position-right
               tj--height-full
               tj-grid-flex
               tj-grid-flex--align-center
               tj--padding-hor-3">
      <div class="tj--border-white-1 tj--border-radius-3">
        <div class="tj-navigation__mob-menu-line tj--border-white-1 tj--margin-hor-1 tj--border-radius-3"></div>
        <div class="tj-navigation__mob-menu-line tj--border-white-1 tj--margin-hor-1 tj--border-radius-3"></div>
        <div class="tj-navigation__mob-menu-line tj--border-white-1 tj--margin-hor-1 tj--border-radius-3"></div>
      </div>
    </div>
    <nav class="tj-navigation__links-mob
                tj--display-none
                tj--position-absolute
                tj--width-full
                tj--font-size-20
                tj--font-weight-light
                tj--border-bottom-white-1">
      <%# navigationLinks %>
      <a href="<% linkAddress %>"
         class="tj-navigation__link-mob
                tj-grid-flex
                tj-grid-flex--justify-center
                tj--display-block
                tj--text-decoration-none
                tj--font-white
                tj--border-bottom-transparent-1">
        <div class="tj-grid-flex
                    tj-grid-flex--align-center
                    tj--position-relative">
          <div>{{ <% linkName %> }}</div>
          <%# isActive %>
          <div class="tj--position-absolute
                      tj--width-full
                      tj--position-left
                      tj--position-bottom
                      tj--border-bottom-white-3"></div>
          <%/ isActive %>
        </div>
      </a>
      <%/ navigationLinks %>
      <div class="tj--position-absolute
                  tj--position-top
                  tj--width-full
                  tj--height-full
                  tj--overflow-hidden
                  tj--z-index-neg-1">
        <div class="tj--navigation__background-nav-links
                    tj--position-absolute
                    tj--width-full"
             style="background-image: url('/img/paris.jpg')"></div>
      </div>
    </nav>
  </div>
  <%^ isOverlay %>
  <div class="tj--position-absolute
              tj--position-top
              tj--width-full
              tj--height-full
              tj--overflow-hidden
              tj--z-index-neg-1">
    <div class="tj--navigation__background
                tj--width-full"
                style="background-image: url('/img/paris.jpg')"></div>
  </div>
  <%/ isOverlay %>
</div>
